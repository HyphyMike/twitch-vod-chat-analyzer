{
  "name": "twitch-vod-chat-analyzer",
  "version": "1.0.0",
  "description": "Twitch Chat and VOD Analyzer with automatic clip generation",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start": "node server/index.js",
    "test": "vitest"
  },
  "keywords": ["twitch", "vod", "chat", "analyzer", "clips", "ffmpeg"],
  "author": "HyphyMike",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "axios": "^1.6.0",
    "ws": "^8.14.2",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "dotenv": "^16.3.1",
    "multer": "^1.4.5-lts.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.0",
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.17.0"
  }
}